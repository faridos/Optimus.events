{% extends "FOSUserBundle::layout.html.twig" %}
{% block stylesheets %}
    <link href="{{asset('jquerysctipttop.css')}}" rel="stylesheet" type="text/css">
    <link href="{{ asset('template/css/form.css') }}" rel="stylesheet">
{% endblock %}
{% block nav %}
    <li>
        <form class="form-inline"  action="{{ path("fos_user_security_check") }}" method="post">
            <input type="hidden" name="_csrf_token" value="{{ fos_csrf_provider.generateCsrfToken('authenticate')}}" />
            <div class="form-group">
                <label class="sr-only" for="email">Email address</label>
                <input type="text" name="_username" value="" required="required" class="form-control round" id="email" placeholder="Email ou Pseudo" autocomplete="on" >
            </div>
            <div class="form-group">
                <label class="sr-only" for="password">Mot de passe</label>
                <input type="password" name="_password" required="required" class="form-control round" id="password" placeholder="Mot de passe" >
            </div>
            <input type="submit" name="submit" class="btn btn-round btn-o btn-green" value="Connexion">
        </form>
    </li>
{% endblock %}   
{% block body %}
    <div id="home-map" ></div>
    <div class="home-caption">

        <div class="home-subtitle" style="">
            <div class="col-md-4 col-md-offset-4">
                {% block fos_user_content %}
                    {% include "FOSUserBundle:Registration:register_content.html.twig" %}
                {% endblock fos_user_content %}	


            </div>

        </div>

    </div>
    <div class="search-panel">
        <form class="form-inline" role="form">
            <div class="form-group">
                <img src="{{ asset('template/images/marker-green.png') }}">"&gt;
            </div>
            <div class="form-group">
                <input style="border-radius:25px" type="text" class="form-control" id="pac-input2" placeholder="Entrer ici votre rÃ©gion" autocomplete="on" size="60">
            </div>
            <div class="form-group">
                <a id="btn-input" class="btn btn-round btn-green btn-m">Ma position actuelle</a></div>
        </form>
    </div>
{% endblock %}
{% block javascripts %}

    <script src="{{asset('datemaske/jquery.maskedinput.min.js')}}" type="text/javascript"></script>
    <script type="text/javascript">
        $(function() {
            $("#fos_user_registration_form_dateNaissance").mask("99/99/9999");
            $("#fos_user_registration_form_tel").mask("(9999) 999-999-999");

        });

    </script>
    
    
   
    <script src="{{ asset('template/js/animation.js') }}" type="text/javascript"></script> 
    <script src="{{ asset('template/js/home.js') }}" type="text/javascript"></script>

{% endblock %}