{% extends "FrontOfficeOptimusBundle::layout.html.twig" %}
{% block body %}
    <div id="wrapper">
        <div id="content" class="" >
            {% block content %}
                {% for flashMessage in app.session.flashbag.get('AjouterAlbum') %}
                    <br/>
                    <div class="alert alert-success">
                        <a class="close" data-dismiss="alert">×</a>
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('AjouterPalmarés') %}
                    <br/>
                    <div class="alert alert-success">
                        <a class="close" data-dismiss="alert">×</a>
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('ModifierPalmarés') %}
                    <br/>
                    <div class="alert alert-success">
                        <a class="close" data-dismiss="alert">×</a>
                        {{ flashMessage }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('modificationAlbum') %}
                    <br/>
                    <div class="alert alert-success">
                        <a class="close" data-dismiss="alert">×</a>
                        {{ flashMessage }}
                    </div>
                {% endfor %}



                <div class="col-lg-12">

                    <div style="margin-top: 2%"></div>
                    <div id="invitation-envoyées" style="display: none;" class="alert alert-success">
                        <a class="close" id="closemessag">×</a>
                        les invitations ont été envoyées 
                    </div>
                    <div class="tabsContainer">
                        <ul class="nav nav-tabs nav-justified" role="tablist">
                            <li class="active"><a href="#env" role="tab" data-toggle="tab"><span class="icon-calendar"></span> Events</a></li>
                            <li class=""><a href="#envparticip" role="tab" data-toggle="tab"><span class="icon-list"></span> Participation</a></li>
                            <li class=""><a href="#clubs" role="tab" data-toggle="tab"><span class="fa fa-users"></span> Clubs</a></li>
                            <li class=""><a href="#calendaree" role="tab" data-toggle="tab"><span class="navIcon fa fa-calendar"></span> Calendrier</a></li>
                            <li class=""><a href="#compititions" role="tab" data-toggle="tab"><span class="icon-trophy"></span> Compétitions</a></li>
                            <li class=""><a href="#palmares" role="tab" data-toggle="tab"><span class="icon-badge"></span> Palmares</a></li>
                        </ul>
                        <div class="tab-content">

                            <ul class="tab-pane fade  active in scrollbar" id="env" >
                                {% if user.compte == 0 and user != app.user %}
                                    <img style="width: 100%" src="{{ asset('prive.png') }}">

                                {% else %}
                                    <div id="supprimer-e" style="display: none;" class="alert alert-danger">
                                        <a class="close" data-dismiss="alert">×</a>
                                        Evenement Supprimer
                                    </div>

                                    {% if user.evenements|length == 0 %}
                                        <div class="alert alert-danger alert-dismissible fade in" role="alert">
                                            <div class="icon"><span class="fa fa-spin fa-spinner"></span></div>
                                            <button type="button" class="close" data-dismiss="alert"></button>
                                            <strong>Rien à afficher dans la catégorie evenément!</strong> Créer un pour commencer.
                                        </div>
                                    {% endif %}
                                    {% if app.user == user %}
                                        <div class="buttonsWrapper col-xs-12">
                                            <a class="btn btn-round btn-default btn-sm rightus" href="{{ path('add-event')}}"><span class="fa icon-plus state"></span> <span class="state">Ajouter </span></a><br>
                                            <br>
                                            <br>
                                        </div>
                                    {% endif %}
                                    {% for env in user.evenements|reverse %}
                                        {% if env.active == 1 %}

                                            <div {% if date(env.dateFin)<= date() %}style="background-color: #ddd"{% endif %} class="userWidget-2 tooltipsContainer col-xs-12 {{env.id}}" >
                                                <div class="tooltipsContainer avatar imagus col-md-4">
                                                    <a href="{{path('show_event',{'id':env.id})}}" class="btn btn-round avatar clubos" data-toggle="tooltip" data-tooltip-class="red" data-placement="right" title="{{env.titre}}">
                                                        {% if env.path == null %}
                                                            <img src="{{ asset('event.png')}}" alt="avatar" ></a>
                                                        {% else %}
                                                        <img src="{{ asset(env.WebPath)}}" alt="avatar" ></a>
                                                    {% endif %}
                                                </div>
                                                <div class="col-md-7">
                                                    <div class="info" style="max-width: 100% !important">
                                                        <div ><a href="{{path('show_event',{'id':env.id})}}" class="name">{{env.titre}}</a></div>
                                                        <div class="title">Créateur : <span class="address">{{ env.createur.nom ~' '~env.createur.prenom }}</span></div>
                                                        <div class="title">Lieu : <span class="address">{{env.lieu}}</span></div>
                                                    </div>
                                                </div>
                                                {#<div class="col-md-4">
                                                <div class="info  col-md-12" >
                                                <div class="title">Text descriptif : <span class="address">{{env.description}}</span></div>
                                                </div>
                                                </div>#}
                                                <div style="margin-top: 1%" align="right">
                                                    <a href="#plusInfo{{env.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="Liste des participant"><span class="icon-users"></span></a>

                                                    {% if app.user == env.createur and date(env.dateFin)>= date()%}

                                                        <a href="#envoyerinvitfriend{{env.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="inviter"><span class="icon-plus"></span></a>
                                                        <a href="{{ path('update-event',{id: env.id})}}"  class="btn btn-icon btn-round btn-green btn-xs" title="modifier"><span class="icon-settings"></span></a>

                                                        <a href="#supprimerEvent{{env.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="supprimer"><span class="icon-trash"></span></a>

                                                    {% endif %}
                                                </div>
                                                {% if date(env.dateFin)<= date() %}
                                                    <div class="label label-danger" style="margin-left: 40%; font-size: 12px; background-color: #c02100 !important;">
                                                        Cette évènement est depassé
                                                    </div>
                                                {% endif %}
                                                <div class="clearfix"></div>
                                            </div>
                                        {% endif %}
                                    {% endfor %}
                                {% endif %}
                            </ul>
                            <ul class="tab-pane fade  scrollbar" id="envparticip" >
                                {% if user.compte == 0 and user != app.user %}
                                    <img style="width: 100%" src="{{ asset('prive.png') }}">

                                {% else %}
                                    {% if user.participations|length == 0 %}
                                        <div class="alert alert-danger alert-dismissible fade in" role="alert">
                                            <div class="icon"><span class="fa fa-spin fa-spinner"></span></div>
                                            <button type="button" class="close" data-dismiss="alert"></button>
                                            <strong>Rien à afficher dans la catégorie participation!</strong> Créer un pour commencer.
                                        </div>
                                    {% endif %}


                                    {% for part in participations|reverse %}

                                        {% if part.event.active == 1 %}
                                            <div {% if date(part.event.dateFin)<= date() %}style="background-color: #ddd"{% endif %}  class="userWidget-2 tooltipsContainer col-md-12 {{part.event.id}}" >
                                                <div class="tooltipsContainer avatar imagus col-md-4">
                                                    <a href="{{path('show_event',{'id':part.event.id})}}" class="btn btn-round avatar clubos" data-toggle="tooltip" data-tooltip-class="red" data-placement="right" title="{{part.event.titre}}">
                                                        {% if part.event.path == null %}
                                                            <img src="{{ asset('event.png')}}" alt="avatar" ></a>
                                                        {% else %}
                                                        <img src="{{ asset(part.event.WebPath)}}" alt="avatar" ></a>
                                                    {% endif %}
                                                </div>
                                                <div class="col-md-7">
                                                    <div class="info" style="max-width: 100% !important">
                                                        <div ><a href="{{path('show_event',{'id':part.event.id})}}" class="name">{{part.event.titre}}</a></div>
                                                        <div class="title">Créateur : <span class="address">{{ part.event.createur.nom ~' '~part.event.createur.prenom }}</span></div>
                                                        <div class="title">Lieu : <span class="address">{{part.event.lieu}}</span></div>
                                                    </div>
                                                </div>
                                                {#<div class="col-md-4">
                                                <div class="info" >
                                                <div class="title">Text descriptif : <span class="address">{{part.event.description}}</span></div>
                                                </div>
                                                </div> #}

                                                <div style="margin-top: 1%" align="right">
                                                    <a href="#plusInfoparticipation{{ part.event.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="Liste des members"><span class="icon-users"></span></a>


                                                    {% if app.user != part.event.createur and app.user == user %}
                                                        {% set participantOuNon=ParticipantOuNon(part.event,app.user) %}
                                                        {% if participantOuNon == 1 and date(part.event.dateFin)>= date() %}
                                                            <a class="btn btn-green btn-round eventus" href="#" id="exit_event{{part.event.id}}">
                                                                <span class="fa icon-action-redo "></span> <span class="exitEvent{{part.event.id}}">Annuler participation</span>                                          
                                                            </a>
                                                            <a class="loader" id="loader_ajouter_relation">
                                                                <img src="{{asset('template/images/ajax-loader-comment.gif') }}">
                                                            </a>
                                                        {% endif %}
                                                    {% endif %}
                                                </div>
                                                {% if date(part.event.dateFin)<= date() %}
                                                    <div class="label label-danger" style="margin-left: 40%; font-size: 12px; background-color: #c02100 !important;">
                                                        Cette évènement est depassé
                                                    </div>
                                                {% endif %}
                                                <div class="clearfix"></div>
                                            </div>
                                        {% endif %}
                                    {% endfor %}
                                {% endif %}
                            </ul>
                            <ul class="tab-pane fade  scrollbar" id="calendaree" >
                                {% if user.compte == 0 and user != app.user %}
                                    <img style="width: 100%" src="{{ asset('prive.png') }}">

                                {% else %}
                                    <div class="buttonsWrapper col-xs-12">
                                        <div id="calendar">
                                        </div>
                                    </div>
                                {% endif %}
                            </ul>
                            <ul class="tab-pane fade scrollbar" id="clubs" >
                                {% if user.compte == 0 and user != app.user %}
                                    <img style="width: 100%" src="{{ asset('prive.png') }}">

                                {% else %}
                                    {% if user.profil == 'Entraineur' %}
                                        <div id="supprimer-c" style="display: none;" class="alert alert-danger">
                                            <a class="close" data-dismiss="alert">×</a>
                                            Club Supprimer
                                        </div>
                                        {% if user.clubs|length == 0 %}
                                            <div class="alert alert-danger alert-dismissible fade in" role="alert">
                                                <div class="icon"><span class="fa fa-spin fa-spinner"></span></div>
                                                <button type="button" class="close" data-dismiss="alert"></button>
                                                <strong>Rien à afficher dans la catégorie clubs!</strong> Créer un pour commencer.
                                            </div>

                                        {% endif %}
                                        {% if app.user == user %}
                                            <div class="buttonsWrapper col-xs-12">
                                                <a class="btn btn-round btn-default btn-sm rightus" href="{{ path('add_club')}}"><span class="fa icon-plus state"></span> <span class="state">Ajouter </span></a><br>
                                                <br>
                                            </div>
                                        {% endif %}
                                        {% for club in user.clubs|reverse %}
                                            {% if club.active == 1 %}
                                                <div {% if club.isPayant == 0 %}style="background-color: #ddd"{% endif %} class="userWidget-2 tooltipsContainer col-md-12 {{club.id}}" >
                                                    <div class="tooltipsContainer avatar imagus">
                                                        <a href="{{path('show_club',{'id':club.id})}}" class="btn btn-round avatar clubos" data-toggle="tooltip" data-tooltip-class="red" data-placement="right" title="{{ club.nom }}">
                                                            {% if club.path == null %}

                                                                <img src="{{ asset('logoclub.png') }}" alt="avatar" ></a>
                                                            {% else %}
                                                            <img src="{{ asset(club.WebPath) }}" alt="avatar" ></a>
                                                        {% endif %}
                                                    </div>
                                                    <div class="col-md-7">
                                                        <div class="info" style="max-width: 100% !important;">
                                                            <div class="name">{{ club.nom}}</div>
                                                            <div class="title">Créateur du club : <span class="address">{{ user.nom ~' '~user.prenom }}</span></div>
                                                            <div class="title">Adresse du club : <span class="address">{{ club.adresse}}</span></div>
                                                            <div class="title">Frais d'abonnement : <span class="address">{{ club.fraisAdhesion}}£</span></div>
                                                        </div>
                                                    </div>

                                                    <div  style="margin-top: 1%" align="right">
                                                        <a href="#plusInfoClub{{club.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="Liste des members"><span class="icon-users"></span></a>
                                                            {% if app.user == club.createur and club.isPayant == 1 %}
                                                            <a href="#inviteramisclub{{club.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="inviter"><span class="icon-plus"></span></a>
                                                            <a href="{{ path('club_update', {'id':club.id})}}" class="btn btn-icon btn-round btn-green btn-xs" title="modifier"><span class="icon-settings"></span></a>

                                                            <a href="#supprimerclub{{club.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="supprimer"><span class="icon-trash"></span></a>
                                                            {% endif %}
                                                    </div>
                                                    <div class="clearfix"></div>
                                                </div>
                                            {% endif %}
                                        {% endfor %}
                                    {% endif %}
                                    {% if user.profil == 'Sportif' %}
                                        {% if user.adherent|length == 0 %}
                                            <div class="alert alert-danger alert-dismissible fade in" role="alert">
                                                <div class="icon"><span class="fa fa-spin fa-spinner"></span></div>
                                                <button type="button" class="close" data-dismiss="alert"></button>
                                                <strong>Rien à afficher dans la catégorie clubs!</strong>
                                            </div>
                                        {% endif %}
                                        {% for member in user.adherent|reverse %}
                                            {% if member.clubad.active == 1 %}
                                                <div {% if member.clubad.isPayant == 0 %}style="background-color: #ddd"{% endif %} class="userWidget-2 tooltipsContainer col-md-12 {{member.clubad.id}}" >
                                                    <div class="tooltipsContainer avatar imagus col-md-2">
                                                        <a href="{{path('show_club',{'id':member.clubad.id})}}" class="btn btn-round avatar clubos" data-toggle="tooltip" data-tooltip-class="red" data-placement="right" title="{{ user.nom ~' '~user.prenom }}">
                                                            {% if member.clubad.path == null %}
                                                                <img src="{{ asset('logoclub.png') }}" alt="avatar" ></a>
                                                            {% else %}
                                                            <img src="{{ asset(member.clubad.WebPath) }}" alt="avatar" ></a>
                                                        {% endif %}

                                                    </div>
                                                    <div class="col-md-5">
                                                        <div class="info">
                                                            <div class="name">{{ member.clubad.nom}}</div>
                                                            <div class="title">Créateur du club : <span class="address">{{ member.clubad.createur.nom ~' '~member.clubad.createur.prenom }}</span></div>
                                                            <div class="title">Adresse du club : <span class="address">{{ member.clubad.adresse}}</span></div>
                                                            <div class="title">Frais d'abonnement : <span class="address">{{ member.clubad.fraisAdhesion}}£</span></div>
                                                        </div>
                                                    </div>


                                                    {% set memberconfirmed = getMembreConfirmed(user,member.clubad) %}
                                                    {% if member.member == app.user %}
                                                        {% if memberconfirmed %}

                                                            <div  class="col-md-3" align="right">
                                                                <a  class="loader" id="loader{{ member.clubad.id }}" >
                                                                    <img src="{{asset('template/images/ajax-loader-comment.gif') }}">
                                                                </a>
                                                                {% if member.confirmed == 1 %}
                                                                    <a   class="btn btn-green btn-round " href="#" id="exit_club{{member.clubad.id}}" >
                                                                        <span  class="fa icon-action-redo"></span>
                                                                        <span  class="exitClub" id="{{member.clubad.id}}">Quitter</span>
                                                                    </a>
                                                                {% elseif member.confirmed == 2 %}
                                                                    <a  class="btn btn-green btn-round " href="#" id="activer_member_club{{member.clubad.id}}" >
                                                                        <span class="fa icon-action-redo "></span>
                                                                        <span  class="activerclub" id="{{member.clubad.id}}" >Regagner</span>
                                                                    </a>
                                                                {% elseif memberconfirmed.confirmed == 0 %}

                                                                    <a  class="btn btn-green btn-round "  href="#"  id="AttentrejoindreClub">

                                                                        <span >En attente</span>
                                                                    </a>
                                                                {% endif %}
                                                            </div>

                                                        {% endif %}
                                                    {% else %}
                                                        <div  class="col-md-3">  
                                                        </div>
                                                    {% endif %}
                                                    <div id="divv{{member.clubad.id}}" class="col-md-3" style="display: none;">
                                                        <a style="display: none;"  class="btn btn-green btn-round "  href="#"  id="AttentrejoindreClub{{ member.clubad.id }}">
                                                            <span >En attente</span>
                                                        </a>
                                                        <a style="display: none" class="btn btn-green btn-round " href="#" id="activer_member_club{{ member.clubad.id }}" >
                                                            <span class="fa icon-action-redo "></span> <span  class="activerclub" id="{{ member.clubad.id }}" >Regagner</span>
                                                        </a>
                                                    </div>
                                                    <div>
                                                        <div class="col-md-1" style="margin-top: 1%" align="right">
                                                            <a href="#plusInfoClubmember{{member.clubad.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="Liste des members"><span class="icon-users"></span></a>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                    </div>
                                                {% endif %}
                                            {% endfor %}
                                        {% endif %}
                                    {% endif %}
                            </ul>
                            <ul class="tab-pane fade  scrollbar" id="compititions" >
                                {% if user.compte == 0 and user != app.user %}
                                    <img style="width: 100%" src="{{ asset('prive.png') }}">

                                {% else %}

                                    {% for club in tousClubs %}
                                        {% set i =0%}
                                        {% if club.active ==1 %}
                                            {% for competition in club.competitions|reverse %}
                                                {% if competition.active == 1 %}
                                                    {% for particip in competition.particips %}
                                                        {% for member in particip.club.adherents %}
                                                            {% for partc in particip.partclubcomp %}
                                                                {% if partc.participant.member == user %}
                                                                    {% set i = 1 %}
                                                                {% endif %}
                                                            {% endfor %}
                                                        {% endfor %}
                                                    {% endfor %}
                                                    {% if i == 1 and competition.active == 1%}
                                                        <div {% if date(competition.dateFin)<= date() %}style="background-color: #ddd"{% endif %} class="userWidget-2 tooltipsContainer col-xs-12 {{competition.id}}" >
                                                            <div class="tooltipsContainer avatar imagus col-md-4">
                                                                <a href="{{path('competition_show',{'id':competition.id})}}" class="btn btn-round avatar clubos" data-toggle="tooltip" data-tooltip-class="red" data-placement="right" title="{{competition.titre}}">
                                                                    {% if competition.path == null %}
                                                                        <img src="{{ asset('logoCompetition.png')}}" alt="avatar" ></a>
                                                                    {% else %}
                                                                    <img src="{{ asset(competition.WebPath)}}" alt="avatar" ></a>
                                                                {% endif %}</div>
                                                            <div class="col-md-6">
                                                                <div class="info" style="max-width: 100% !important">
                                                                    <div ><a href="{{path('competition_show',{'id':competition.id})}}" class="name">{{competition.titre}}</a></div>
                                                                    <div class="title"><span class="address"> Du {{ competition.dateDebut|date('d-m-Y H:i:s') ~' À '~competition.dateFin|date('d-m-Y H:i:s') }}</span></div>
                                                                    <div class="title">Lieu : <span class="address">{{competition.lieu}}</span></div>
                                                                </div></div>
                                                            <div style="margin-top: 1%" align="right">
                                                                <a href="#plusInfoCompetition{{competition.id}}" data-toggle="modal" class="btn btn-icon btn-round btn-green btn-xs" title="Liste des participant"><span class="icon-users"></span></a>

                                                            </div>
                                                            {% if date(competition.dateFin)<= date() %}
                                                                <div class="label label-danger" style="margin-left: 40%; font-size: 12px; background-color: #c02100 !important;">
                                                                    Cette compétition est depassé
                                                                </div>
                                                            {% endif %}
                                                            <div class="clearfix"></div>
                                                        </div>

                                                    {% endif %}
                                                {% endif %}
                                            {% endfor %}
                                        {% endif %}
                                    {% endfor %}
                                {% endif %}

                            </ul>

                            <ul class="tab-pane fade scrollbar" id="palmares" >
                                <div id="supprimer-p" style="display: none;" class="alert alert-danger">
                                    <a class="close" data-dismiss="alert">×</a>
                                    Palmarés Supprimer
                                </div>
                                {% if user.reward|length == 0 %}
                                    <div class="alert alert-danger alert-dismissible fade in" role="alert">
                                        <div class="icon"><span class="fa fa-spin fa-spinner"></span></div>
                                        <button type="button" class="close" data-dismiss="alert"></button>
                                        <strong>Rien à afficher dans la catégorie palmares!</strong> Créer un pour commencer.
                                    </div>
                                {% endif%}
                                {% if app.user == user %}
                                    <div class="buttonsWrapper col-md-12">
                                        <a href="#uploadpalmares" data-toggle="modal" class="btn btn-round btn-default btn-sm rightus"><span class="fa icon-plus state"></span> Ajouter</a>
                                        <br>
                                    </div>
                                {% endif%}
                                <div class="row">
                                    <br>
                                    {% for palmares in user.reward|reverse %}
                                        <div  class="row {{palmares.id}}">
                                            <br>
                                            <div class="col-md-12" id="block{{ palmares.id }}">
                                                <div style="background-color: {{palmares.couler}}" data-linkto="" class="card albumus tooltipsContainer" >
                                                    <br>
                                                    <div class="profile-card"style="margin-right: 1%;">
                                                        <div class="">
                                                            {% if app.user == user %}
                                                                <div align="right">
                                                                    <a href="#modifierpalmares_{{palmares.id}}" class="btn btn-icon btn-round btn-green btn-xs" title="modifier" data-toggle="modal" data-tooltip-class="red" data-placement="bottom"><span class="icon-settings"></span></a>
                                                                    <a href="#supprimerpalmares_{{palmares.id}}" class="btn btn-icon btn-round btn-green btn-xs" title="supprimer" data-toggle="modal" data-tooltip-class="red" data-placement="bottom"><span class="icon-trash"></span></a>
                                                                </div>
                                                            {% endif %}
                                                            <div class="clearfix"></div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="pc-avatar">
                                                                <a href="#" class="btn btn-icon btn-round btn-o palmaruss" title="{{ palmares.titre }}" data-toggle="tooltip" data-tooltip-class="red" data-placement="top" >
                                                                    <span class=""><img src="{{ asset('titre.png') }}"></span></a>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="pc-avatar">
                                                                <a href="#" class="btn btn-icon btn-round btn-o  palmaruss" title="{{ palmares.classment }}" data-toggle="tooltip" data-tooltip-class="red" data-placement="top">
                                                                    <span ><img src="{{ asset('classement.png') }}"></span></a>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">

                                                            <div class="profile-card">
                                                                <div class="pc-avatar">
                                                                    <a href="#" class="btn btn-icon btn-round btn-o  palmaruss" title="{{ palmares.date |date('Y-m-d')}}" data-toggle="tooltip" data-tooltip-class="red" data-placement="top">
                                                                        <span ><img src="{{ asset('date.png') }}"></span></a>
                                                                </div>
                                                            </div>


                                                            <br>

                                                            <div class="clearfix"></div>

                                                        </div>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                </div>


                                            </div>


                                        </div>
                                    {% endfor %}
                                </div>

                            </ul>
                            <div class="clearfix"></div>



                        </div>
                    </div>

                </div>
