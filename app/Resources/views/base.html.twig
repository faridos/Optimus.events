        <script src="{{ asset('template/js/jquery.easing.min.js') }}"  type="text/javascript"></script>  
        <script type="text/javascript" src="{{ asset('template/js/selectinput.js') }}"></script>
        <script src="{{ asset('template/js/jquery.datetimepicker.js') }}"></script>
        <script src="{{ asset('template/js/datus.js') }}"></script>


        <script src="{{ asset('template/js/jquery.slimscroll.min.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery.tagsinput.min.js') }}"  type="text/javascript"></script>

        <script src="{{ asset('template/js/bootstrap-datepicker.js') }}"  type="text/javascript"></script>
        {% block javascripts %}
        {% endblock %}
    </body>
</html>
