<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
        <title>{% block title %}Optimus{% endblock %}</title>
        <link rel="stylesheet" type="text/css" href="{{ asset('template/css/jquery.datetimepicker.css') }}"/>
        <link href="{{ asset('template/css/font-awesome.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/optimus.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/simple-line-icons.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/fullscreen-slider.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/app.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('template/css/form.css') }}" type="text/css" rel="stylesheet" />
        
        <link href="{{ asset('template/css/jquery-ui.css') }}" rel="stylesheet">
        <link rel="icon" type="image/x-icon" href="{{ asset('favo.ico') }}" />

        {% block stylesheets %}

        {% endblock %}

    </head>
    <body class="notransition no-hidden">
        <div id="hero-container-map" >

            <!-- Hero -->
            {% block navigation %}
            {% endblock %}


            {% block body %}
            {% endblock %}
        </div>
        {% block footer %}
        {% endblock %}
        
        
        <script src="{{ asset('template/js/app.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery-2.1.1.min.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery-ui.min.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery-ui-touch-punch.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery.placeholder.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/bootstrap.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery.touchSwipe.min.js') }}"  type="text/javascript"></script>
        <script src="http://maps.googleapis.com/maps/api/js?sensor=true&amp;libraries=geometry&amp;libraries=places" type="text/javascript"></script>
        <script src="{{ asset('template/js/infobox.js') }}"  type="text/javascript"></script>
        <script src="{{ asset('template/js/jquery.visible.js') }}"  type="text/javascript"></script>
        <!--script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script-->
